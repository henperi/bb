        <!-- begin #content -->
		<div id="content" class="content">
			<!-- begin breadcrumb -->
			<ol class="breadcrumb pull-right">
				<li class="breadcrumb-item"><a href="javascript:;">Home</a></li>
				<li class="breadcrumb-item"><a href="javascript:;">Dashboard</a></li>
				<li class="breadcrumb-item active">Dashboard v2</li>
			</ol>
			<!-- end breadcrumb -->
			<!-- begin page-header -->
			<h1 class="page-header">{{user.role}}'s Dashboard</small></h1>{{admin.firstname}}
			{{> flash}}
			<!-- end page-header -->
			<!-- begin row -->
			<div class="row">
			    <!-- begin col-3 -->
				
			    <div class="col-lg-4 col-md-6">
			        <div class="widget widget-stats bg-gradient-teal">
						<div class="stats-icon stats-icon-lg"><i class="fa fa-globe fa-fw"></i></div>
			        	<div class="stats-content">
							<div class="stats-title">BEWLA BALLANCE</div>
							<div class="stats-number"> &#8358; {{formatCash adminWallet.ballance}}</div>
							{{#if mainAdmin}}				
							<div class="stats-desc"><a href="#modal-dialog" class="btn btn-white btn-lg btn-block" data-toggle="modal">Generate Fund</a></div>
							{{/if}}
							<!-- #modal-dialog -->
							<div class="modal fade" id="modal-dialog">
								<div class="modal-dialog">
									<div class="modal-content">
										<div class="modal-header">
											<h4 class="modal-title">Generate Fund</h4>
											<button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
										</div>
										<div class="modal-body">
											<form action="admins/gen-funds" method="POST" class="margin-bottom-0">
												<div class="form-group m-b-20">
													<label for="remember_checkbox">
														Enter Amount 
													</label>
													<input type="number" name="gen_amount" class="form-control form-control-lg" placeholder="Enter Amount" required />
												</div>
												<div class="form-group m-b-20">
													<label for="remember_checkbox">
														Type a remark for this generation 
													</label>
													<textarea name="remark" class="form-control form-control-lg" placeholder="Enter remark for this funds generation" rows="3"></textarea>
												</div>
												<div class="checkbox checkbox-css m-b-20">
													<input type="checkbox" id="remember_checkbox" /> 
													<label for="remember_checkbox">
														Acknowledge action 
													</label>
												</div>
												<div class="login-buttons">
													<button type="submit" class="btn btn-success btn-block btn-lg">Generate</button>
												</div>
											</form>
										</div>
										<div class="modal-footer">
											<a href="javascript:;" class="btn btn-white btn-block btn-lg" data-dismiss="modal">Cancel</a>
										</div>
									</div>
								</div>
							</div>
						</div>
			        </div>
			    </div>
				
			    <!-- end col-3 -->

				
			    <!-- begin col-3 -->
				
			    <div class="col-lg-4 col-md-6">
			        <div class="widget widget-stats bg-gradient-teal">
						<div class="stats-icon stats-icon-lg"><i class="fa fa-globe fa-fw"></i></div>
			        	<div class="stats-content">
							<div class="stats-title">RECEIVED COMMISSIONS</div><span class="capped"></span>
							<div class="stats-number"> &#8358; {{commissionsWallet.ballance}}</div>
							{{#if mainAdmin}}
								<div class="stats-desc m-t-5">
									<a href="" class="btn btn-default btn-md m-r-20 m-b-5" data-toggle="modal">History</a>
									<a href="#commissions" class="btn btn-primary btn-md m-r-20" data-toggle="modal">Commission Settings</a>
								</div>
							{{/if}}
							
							<!-- #modal-dialog -->
							<div class="modal fade" id="commissions">
								<div class="modal-dialog">
									<div class="modal-content">
										<div class="modal-header">
											<h4 class="modal-title">Commissions Settings</h4>
											<button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
										</div>
										<div class="modal-body">
											<form action="admins/gen-funds" method="POST" class="margin-bottom-0">
												<h4>Charge on Admins</h4>
												<div class="row">
													<div class="col-md-6">
														<div class="form-group  m-b-20">
															<label for="remember_checkbox">
																Percentage(%) Charge on Agent 
															</label>
															<input type="text" name="percentChargeOnAdmins" class="form-control input-percent-agent" value="{{ commissionsData.on_agent.percent }}" placeholder="Enter Amount" required />
															<span class="text-danger percent-error-agent"> </span>
														</div>
													</div>
													<div class="col-md-6">
														<div class="form-group  m-b-20">
															<label for="remember_checkbox">
																Cap/Limit on Agent(&#8358;)
															</label>
															<input type="text" name="cappedChaargeOnAdmins" class="form-control input-capped-agent" value="{{ commissionsData.on_agent.capped }}" placeholder="Enter Amount" required />
															<span class="text-danger capped-error-agent"> </span>
														</div>
													</div>
												</div>
												<div class="col-xs-12">
													<h4>Description</h4>

													<div class="col-xs-12">
														<h5 class="m-b-0">1. When Admin funds Agent</h5>
														<p class="text-dark">When a <strong>Company Staff (Main-Admin/Manager)</strong> funds an Agent a 
															<strong><span class="percentChargeOnAgent">{{ commissionsData.on_agent.percent }}</span></strong>% commission capped at 
															<strong>&#8358;<span class="cappedChargeOnAgent">{{ commissionsData.on_agent.capped }}</span></strong> will be charged on the Agent and added to the
															<strong>Company Staff's</strong> ballance.
														</p>
													</div>
												</div>
												<hr class="text-info">

												<h4>Charge on Users</h4>
												<div class="row">
													<div class="col-md-6">
														<div class="form-group  m-b-20">
															<label for="remember_checkbox">
																Percentage Charge(%) on Users
															</label>
															<input type="text" name="percentCharge" class="form-control input-percent" value="{{ commissionsData.standard.percent }}" placeholder="Enter Amount" required />
															<span class="text-danger percent-error"> </span>
														</div>
													</div>
													<div class="col-md-6">
														<div class="form-group  m-b-20">
															<label for="remember_checkbox">
																Cap/Limit (&#8358;) on Users
															</label>
															<input type="text" name="cappedChaarge" class="form-control input-capped" value="{{ commissionsData.standard.capped }}" placeholder="Enter Amount" required />
															<span class="text-danger capped-error"> </span>
														</div>
													</div>
												</div>
												<div class="col-xs-12">
													<h4>Description</h4>

													<div class="col-xs-12">
														<h5 class="m-b-0">1. When Admin funds User</h5>
														<p class="text-dark">When a <strong>Company Staff(Main-Admin or Manager)</strong> funds a User a 
															<strong><span class="percentCharge">{{ commissionsData.standard.percent }}</span></strong>% commission capped at 
															<strong>&#8358;<span class="cappedCharge">{{ commissionsData.standard.capped }}</span></strong> will be charged on the User and added to the
															<strong>Company Staff's</strong> ballance.
														</p>
													</div>

													<div class="col-xs-12">
														<h5 class="m-b-0">2. When Agent funds User</h5>
														<p class="text-dark">When an <strong>Agent</strong> funds a User a 
															<strong><span class="percentCharge">{{ commissionsData.standard.percent }}</span></strong>% commission capped at 
															<strong>&#8358;<span class="cappedCharge">{{ commissionsData.standard.capped }}</span></strong> will be charged on the User.
															<strong>60%</strong> of this commission will be added to Bewla's <strong>Main Admin's</strong> ballance while 
															<strong>40%</strong> will be added to the <strong>Agent's</strong> ballance.
														</p>
													</div>
												</div>
												<div class="login-buttons">
													<button type="submit" class="btn btn-success btn-block btn-lg">Update Commission Charges</button>
												</div>
											</form>
										</div>
										<div class="modal-footer">
											<a href="javascript:;" class="btn btn-white btn-block btn-lg" data-dismiss="modal">Cancel</a>
										</div>
									</div>
								</div>
							</div>

							{{#unless mainAdmin}}
								<div class="stats-desc"><a href="" class="btn btn-secondary btn-lg btn-block" data-toggle="modal">Commissions History</a></div>
							{{/unless}}
						</div>
			        </div>
			    </div>
				
			    <!-- end col-3 -->
				
			</div>
			<!-- end row -->

			<div class="row">
				
			</div>
			
			<!-- begin row -->
			{{#if companyStaff}}
			<div class="row">
				<div class="col-lg-8">
					<div class="panel panel-inverse" data-sortable-id="ui-widget-2">
						<div class="panel-heading ui-sortable-handle">
							<div class="btn-group pull-right">
								<button type="button" class="btn btn-success btn-xs">Users</button>
								<button type="button" class="btn btn-success btn-xs dropdown-toggle" data-toggle="dropdown">
								</button>
								<ul class="dropdown-menu" role="menu">
									<li><a href="admins/accounts/users">Users</a></li>
									<li><a href="admins/accounts/agents">Agents</a></li>
									{{#if mainAdmin}}
									<li><a href="admins/accounts/managers">Managers</a></li>
									{{/if}}
									<li class="divider"></li>
									<li><a href="javascript:;">All Accounts</a></li>
								</ul>
							</div>
							<h4 class="panel-title">Registered Users</h4>
						</div>
						<div class="panel-body">
                        	<!-- begin table-responsive -->
                        	<div class="table-responsive">
								<table class="table table-striped m-b-0">
									<thead>
										<tr>
											<th>#</th>
											<th>Name</th>
											<th>Mobile</th>
											<th>Email Address</th>
											<th>Account Type</th>
											<th>Ballance</th>
											<th></th>
										</tr>
									</thead>
									<tbody>
										{{#each usersData }}
											<tr>
												<td class="with-img">
													<img src="../assets/img/user/user-1.jpg" class="img-rounded height-30" />
												</td>
												<td>{{ this.firstname }} {{ this.lastname }}</td>
												<td>{{ this.mobile }}</td>
												<td>{{ this.email }}</td>
												<td>{{ this.account_type }}</td>
												<td>{{this.wallets.[0].ballance}}</td>
												<td class="with-btn" nowrap>
													<a href="#edit_{{this.mobile}}" class="btn btn-sm btn-primary width-60 m-r-2" data-toggle="modal">Fund</a>
													<a href="admins/edit/{{this.account_type}}/{{this.email}}" class="btn btn-sm btn-info width-60 m-r-2">Edit</a>
													<a href="javascript:;" class="btn btn-sm btn-warning width-60 m-r-2">Block</a>
												</td>
												
												<td>
													<!-- #modal-dialog -->
													<div class="modal fade" id="edit_{{this.mobile}}">
														<div class="modal-dialog">
															<div class="modal-content">
																<div class="modal-header">
																	<h4 class="modal-title">Fund {{ this.firstname }} <span class="badge badge-info">{{ this.account_type }}</span></h4>
																	<button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
																</div>
																<div class="modal-body">
																	<form action="admins/fund/accounts/{{ this.account_type }}" method="POST" class="margin-bottom-0">
																		<div class="form-group m-b-20">
																			<label for="enter_amount">
																				Enter Amount 
																			</label>
																			<input type="float" name="fund_amount" class="form-control form-control-lg" placeholder="Enter Amount" required />
																		</div>
																		<div class="form-group m-b-20">
																			<label for="enter_amount">
																				Enter Commission 
																			</label>
																			<input type="float" name="com_to_admin" class="form-control form-control-lg" placeholder="Enter Commission, Default is 3% for users or 5% for agents" />
																		</div>

																		<div class="form-group m-b-20 d-none">
																			<input type="text" name="account_type" class="form-control form-control-lg" value="{{ this.account_type }}" required />
																			<input type="text" name="wallet_id" class="form-control form-control-lg" value="{{ this.wallets.[0].wallet_id }}" required />
																			<input type="text" name="u_id" class="form-control form-control-lg" value="{{ this._id }}" required />
																		</div>

																		<div class="form-group m-b-20">
																			<label for="remember_checkbox">
																				Type a remark for this funding (optional) 
																			</label>
																			<textarea name="remark" class="form-control form-control-lg" placeholder="Enter remark for this funds generation" rows="3"></textarea>
																		</div>
																		<div class="checkbox checkbox-css m-b-20">
																			<input type="checkbox" id="remember_checkbox" /> 
																			<label for="remember_checkbox">
																				Acknowledge action 
																			</label>
																		</div>
																		<div class="login-buttons">
																			<button type="submit" class="btn btn-success btn-block btn-lg">Fund {{ this.account_type }}</button>
																		</div>
																	</form>
																</div>
																<div class="modal-footer">
																	<a href="javascript:;" class="btn btn-white btn-block btn-lg" data-dismiss="modal">Cancel</a>
																</div>
															</div>
														</div>
													</div>
												</td>
											</tr>
											
										{{/each}}
									</tbody>
								</table>								
							</div>
							<div class="panel-footer text-center">
								<a href="javascript:;" class="text-inverse btn btn-default">View All</a>
							</div>
							<!-- end table-responsive -->
						</div>
						<div class="hljs-wrapper">
						
						</div>
					</div>
				</div>
			    <!-- begin col-4 -->
			    <div class="col-lg-4">
			        <!-- begin panel -->
			        <div class="panel panel-inverse" data-sortable-id="index-4">
			            <div class="panel-heading">
			                <h4 class="panel-title">Newly Registered Users <span class="pull-right label bg-gradient-teal">24 new users</span></h4>
			            </div>
                        <ul class="registered-users-list clearfix">
							{{#each usersData }}
								<li>
									<a href="javascript:;"><img src="./assets/img/user/user-12.jpg" alt="" /></a>
									<h4 class="username text-ellipsis">
										{{ this.firstname }}
										<small>{{ this.mobile }}</small>
										{{!-- <a href="javascript:;" class="btn btn-default btn-sm btn-block">View</a> --}}
										{{!-- <small>{{this.wallets.[0].ballance}}</small> --}}
									</h4>
								</li>
							{{/each}}
                        </ul>
			            <div class="panel-footer text-center">
			                <a href="javascript:;" class="text-inverse">View All</a>
			            </div>
			        </div>
			        <!-- end panel -->
			    </div>
			    <!-- end col-4 -->
			</div>
			{{/if}}
			<!-- end row -->
		</div>
		<!-- end #content -->

		